<!-- VISUALIZATIONS-->

<!-- http://linda-project.eu/linda-visualization#BarChart -->

    <owl:NamedIndividual rdf:about="http://linda-project.eu/linda-visualization#BarChart">
        <rdfs:comment xml:lang="en">A graph used to display and compare the number, frequency or other measure for different discrete categories of data.</rdfs:comment>
        <rdf:type rdf:resource="http://linda-project.eu/linda-visualization#Visualization"/>
        <linda-visualization:visualizationType rdf:resource="http://linda-project.eu/linda-visualization#Comparison"/>
        <linda-visualization:visualizationType rdf:resource="http://linda-project.eu/linda-visualization#PartToWhole"/>
        <linda-visualization:visualizationType rdf:resource="http://linda-project.eu/linda-visualization#TimeSeries"/>
        <linda-visualization:structureOption>
           <rdf:Description>
	     <rdf:type rdf:resource="http://linda-project.eu/linda-visualization#Option"/>
		<linda-visualization:optionType>
		   <rdf:Description>
	                <rdf:type rdf:resource="http://linda-project.eu/linda-visualization#Dimension"/>
			<linda-visualization:scaleOfMeasurement rdf:resource="http://linda-project.eu/linda-visualization#Nominal"/>
			<linda-visualization:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</linda-visualization:minCardinality>
			<linda-visualization:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</linda-visualization:maxCardinality>
	            </rdf:Description>
	       </linda-visualization:optionType>
	       <linda-visualization:optionName>Horizontal Axis</linda-visualization:optionName>	
            </rdf:Description>
	</linda-visualization:structureOption>
        <linda-visualization:structureOption>
 	    <rdf:Description>
	     <rdf:type rdf:resource="http://linda-project.eu/linda-visualization#Option"/>
		<linda-visualization:optionType>
		   <rdf:Description>
	                <rdf:type rdf:resource="http://linda-project.eu/linda-visualization#Dimension"/>
			<linda-visualization:scaleOfMeasurement rdf:resource="http://linda-project.eu/linda-visualization#Ratio"/>
			<linda-visualization:scaleOfMeasurement rdf:resource="http://linda-project.eu/linda-visualization#Interval"/>
			<linda-visualization:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</linda-visualization:minCardinality>
			<linda-visualization:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</linda-visualization:maxCardinality>
	            </rdf:Description>
	       </linda-visualization:optionType>
	       <linda-visualization:optionName>Vertical Axis</linda-visualization:optionName>	
            </rdf:Description>
	</linda-visualization:structureOption>
        <linda-visualization:structureOption>
 	   <rdf:Description>
	     <rdf:type rdf:resource="http://linda-project.eu/linda-visualization#Option"/>
		<linda-visualization:optionType>
		   <rdf:Description>
	                <rdf:type rdf:resource="http://linda-project.eu/linda-visualization#Dimension"/>
			<linda-visualization:scaleOfMeasurement rdf:resource="http://linda-project.eu/linda-visualization#Nominal"/>
			<linda-visualization:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</linda-visualization:maxCardinality>
	            </rdf:Description>
	       </linda-visualization:optionType>
	       <linda-visualization:optionName>Groups</linda-visualization:optionName>	
            </rdf:Description>
	</linda-visualization:structureOption>
        <linda-visualization:layoutOption>
           <rdf:Description>
	     <rdf:type rdf:resource="http://linda-project.eu/linda-visualization#Option"/>
	       <linda-visualization:optionType rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
	       <linda-visualization:optionName>Label</linda-visualization:optionName>		
