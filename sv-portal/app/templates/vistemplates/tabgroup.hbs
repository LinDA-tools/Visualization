<div class="row left-collapse">
    <div class="large-2 medium-2 columns">
        <h6 class="workflowStepTitle"  style="background-color: #eeeeee; margin-left: 0.5rem; padding: 0.17rem;"> Select class </h6>
        {{view App.ButtonGroupView  
            content=controller.dataInfo
            selection=controller.dataSubset
        }}
                  
    </div>    
    <div class="stage large-10 medium-10 columns" style="background-color: #ffffff; padding-top:0rem;">
         <h6 class="workflowStepTitle"  style="background-color: #eeeeee; margin-left: 0.5rem; padding: 0.17rem;"> Map properties to dimensions</h6>
        <ul class="list" style="background-color: #ffffff">
            {{#each childView in view.childrenViews}}
            {{view childView}}
            {{/each}}
        </ul>
    </div>  
</div>       