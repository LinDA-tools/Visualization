<!-- http://linda-project.eu/linda-visualization#LineChart -->

    <owl:NamedIndividual rdf:about="http://linda-project.eu/linda-visualization#LineChart">
        <rdfs:comment xml:lang="en">A graph used to visualize a trend in data over intervals of time. Line charts emphasize time flow and rate of change rather than the amount of change.</rdfs:comment>
        <rdf:type rdf:resource="http://linda-project.eu/linda-visualization#Visualization"/>
        <linda-visualization:visualizationType rdf:resource="http://linda-project.eu/linda-visualization#TimeSeries"/>
