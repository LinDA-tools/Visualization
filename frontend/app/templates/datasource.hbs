<div class="row box">
    <div class="large-5 medium-3 columns">
        <!-- Data Selection -->        
        <div class="row inner_box">
            <div class="inner_title_box">
                <h7>EXPLORE AND SELECT DATA</h7>
            </div>
            <div>
                {{tree-selection treedata=controller.treeContent}} 
            </div>
        </div>                                                            
    </div>                                         
  
    <div class="large-7 medium-9 columns">       
        <div class="inner_box">
            <!-- Data Selection Preview -->
            <div class="inner_title_box">
                <h7>PREVIEW SELECTED DATA</h7> <a class="right" {{action 'visualize'}}>Visualize</a>
            </div>
            <div>  
               {{data-table list=controller.tableContent}} 
            </div>         
        </div>
    </div>   
                
</div>