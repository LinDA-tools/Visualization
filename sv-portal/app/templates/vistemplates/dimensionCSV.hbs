<ul class="list" style="font-size: 9pt; list-style-type: none;">
    <li style="width:100%; font-size:10pt; font-weight:bold;">{{view.label}}</li>
    <li style="width:100%">
        {{view App.Select viewName="select"
                size=controller.dimensions.length
                content=controller.dimensions
                optionLabelPath="content.name"
                optionValuePath="content.id"
                selection=view.content}}
    </li>
</ul>
